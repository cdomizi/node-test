{
  "name": "node-test",
  "version": "1.0.0",
  "description": "Dockerized TS Node/Express REST API with PostgreSQL database.",
  "repository": "git@bitbucket.org:cdomizi/node-test.git",
  "author": "Cesare Domizi <domizicesare@gmail.com>",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "prestart": "yarn build",
    "start": "NODE_ENV=production node dist/index.js",
    "dev": "NODE_ENV=development nodemon -L",
    "db-generate": "rm -rf ./src./.prisma && yarn pnpify prisma generate",
    "db-migrate-dev": "yarn pnpify prisma migrate dev"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.1.5",
    "@types/pg": "^8.6.6",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "bcrypt": "^5.1.1",
    "eslint": "^8.40.0",
    "nodemon": "^2.0.22",
    "prisma": "5.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@prisma/client": "5.1.1",
    "@yarnpkg/pnpify": "^4.0.0-rc.49",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.2.1",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.1",
    "morgan": "^1.10.0",
    "pg": "^8.11.0",
    "rimraf": "^5.0.0"
  },
  "packageManager": "yarn@3.6.1"
}
