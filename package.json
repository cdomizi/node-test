{
  "name": "node-test",
  "version": "1.1.0",
  "description": "Dockerized TS Node/Express REST API with PostgreSQL database.",
  "repository": "git@bitbucket.org:cdomizi/node-test.git",
  "author": "Cesare Domizi <domizicesare@gmail.com>",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "ts-lint": "tsc",
    "build": "rimraf dist && npx tsc",
    "prestart": "yarn build",
    "start": "NODE_ENV=production node dist/index.js",
    "dev": "NODE_ENV=development nodemon -L",
    "db-generate": "rm -rf ./src./.prisma && yarn pnpify prisma generate",
    "db-migrate-dev": "yarn pnpify prisma migrate dev"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/http-errors": "^2.0.4",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "bcrypt": "^5.1.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "nodemon": "^3.0.2",
    "prettier": "3.1.1",
    "prisma": "5.7.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@prisma/client": "5.7.1",
    "@yarnpkg/pnpify": "^4.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "rimraf": "^5.0.5"
  },
  "packageManager": "yarn@4.0.2"
}
