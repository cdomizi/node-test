generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model customers {
  id        Int     @id @default(autoincrement())
  firstname String? @db.VarChar(255)
  lastname  String? @db.VarChar(255)
  address   String? @db.VarChar(255)
  email     String? @db.VarChar(255)
}
